{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target":"es2016",                   
    "module":"commonjs",                         
    "esModuleInterop": true,                            
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                               
    "skipLibCheck": true,
    "esModuleInterop": true,                              
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
